<div class="row">
  <div class="col-md-12">
    <table class="table table-bordered table-striped">
      <thead>
        <tr>
          <th>Description</th>
          <th>Amount</th>
          <th>Type</th>
          <th>Account</th>
          <th>Created date</th>
        </tr>
      </thead>
      <tbody>
        {{#each model as |movement|}}
          <tr>
            <td>{{movement.description}}</td>
            <td>{{movement.amount}}</td>
            <td>{{movement.type}}</td>
            <td>{{movement.account}}</td>
            <td>{{movement.created_at}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>